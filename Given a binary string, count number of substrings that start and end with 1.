#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define db double 
#define z long long int 
using namespace std;
int main()
{

       string s ;
       cin>>s;
       int c=0;
       int n = s.length();
       for(int i=0;i<n;i++)
       {
            if(s[i]=='1') c++;
       }
       pf("%d",(c*(c-1))/2 );
   }
